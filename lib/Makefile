all:
	make -C ./NCURSES
	make -C ./SDL
	make -C ./SFML

clean:
	make clean -C ./NCURSES
	make clean -C ./SDL
	make clean -C ./SFML

fclean:	clean
	make fclean -C ./NCURSES
	make fclean -C ./SDL
	make fclean -C ./SFML

re: fclean all
